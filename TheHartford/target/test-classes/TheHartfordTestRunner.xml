<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="The Hartford Automation Test" parallel="false">
    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome-options"/>
    <parameter name="browserVersion" value="8gi5.0"/>
    <parameter name="url" value="https://www.thehartford.com/"></parameter>

    <test name="The Hartford Home">
        <classes>
            <class name="getAQuoteAuto.GetAQuoteAutoTest"></class>
            <class name="getAQuoteBusiness.GetAQuoteBusinessTest"></class>
            <class name="getAQuoteHome.GetAQuoteHomeTest"></class>
            <class name="myAccount.MyAccountTest"></class>
        </classes>

    </test>


</suite>