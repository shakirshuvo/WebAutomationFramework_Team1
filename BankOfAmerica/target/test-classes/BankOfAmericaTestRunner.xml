<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Bank of America Automation Test" parallel="false">
    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="catalina"/>
    <!--    <parameter name="os" value="Windows"/>-->
<!--    <parameter name="os_version" value="10"/>-->
    <parameter name="browserName" value="chrome-options"/>
    <parameter name="browserVersion" value="85"/>
    <parameter name="url" value="https://www.bankofamerica.com/"></parameter>

    <test name="Bank of America">
        <classes>
            <class name="testSearchDataDriven.TestSearch"></class>
            <class name="home.HomePageTest"></class>
            <!--            <class name="myAccount.MyAccountTest"></class>-->
        </classes>

    </test>
</suite>