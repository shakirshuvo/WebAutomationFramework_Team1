<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Amazon Automation Test" parallel="false">
    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="catalina"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="85.0"/>
    <parameter name="url" value="https://www.newyorktimes.com/"></parameter>

    <test name="NewYorkTimes Home">
        <classes>
          <class name="homeTest.HomePageTest"></class>
        </classes>

    </test>

</suite>