<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="NY Times Automation Test" parallel="false">
    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome-options"/>
    <parameter name="browserVersion" value="85.0"/>
    <parameter name="url" value="https://www.nytimes.com/"></parameter>

    <test name="New York Times Home">
        <classes>

                        <class name="home.NYTHomePageTest"></class>
        </classes>

    </test>

</suite>