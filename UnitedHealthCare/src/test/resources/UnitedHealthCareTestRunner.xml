<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="United Health Care Automation Test" parallel="false">
    <parameter name="url" value="https://www.uhc.com/"></parameter>
        <parameter name="useCloudEnv" value="false"/>
        <parameter name="cloudEnvName" value="browserstack"/>
        <parameter name="os" value="Windows"/>
        <parameter name="os_version" value="10"/>
        <!--    <parameter name="browserName" value="chrome-options"/>-->
        <parameter name="browserName" value="chrome"/>

        <parameter name="browserVersion" value="85.0"/>

    <test name="United Health Care Home">
        <classes>
            <class name="home.UnitedHomePageTest"></class>
        </classes>

    </test>

</suite>